{
  "name": "ranwall",
  "version": "2.0.1",
  "description": "App for set random wallpapers",
  "homepage": "./",
  "repository": "https://github.com/jaumesegarra/ranwall2-app",
  "main": "public/electron.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "predist": "npm run build",
    "dist": "electron-builder",
    "preship": "npm run build",
    "ship": "build -mw -p always"
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "delete": "^1.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.5",
    "jimp": "^0.16.1",
    "mkdirp": "^1.0.4",
    "pify": "^5.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/helper-validator-identifier": "^7.10.4",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "concurrently": "^4.1.0",
    "css-loader": "^5.0.1",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron": "^11.0.0-beta.20",
    "electron-builder": "^22.9.1",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.1.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "line-awesome": "^1.0.2",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "^5.0.2",
    "react": "^16.8.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.8.2",
    "react-redux": "^5.1.1",
    "reactn": "^0.2.2",
    "redux": "^4.0.1",
    "resolve": "1.8.1",
    "rxjs": "^6.3.3",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "wait-on": "^3.2.0",
    "webpack": "^4.27.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "author": "Jaume Segarra <jaumesv.jmr@gmail.com>",
  "license": "ISC",
  "private": true,
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "MAIN_WINDOW": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "jaumesegarra.ranwall",
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "package.json",
      {
        "from": "build",
        "to": "public",
        "filter": [
          "!tmpAppFolder",
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "resources"
    },
    "nsis": {
      "perMachine": true
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "mac": {
      "target": {
        "target": "dir",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "artifactName": "${productName}-${arch}-${version}.${ext}",
      "category": "public.app-category.utilities",
      "publish": [
        "github"
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
